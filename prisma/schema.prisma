generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model sunlight {
  id          Int    @id @default(autoincrement())
  auckland    Int
  brisbane    Int
  melbourne   Int
  tokyo       Int
  copenhagen  Int
  toronto     Int
  shanghai    Int
  jakarta     Int
  moscow      Int
  seoul       Int
  istanbul    Int
  cairo       Int
  osaka       Int
  kyoto       Int
  madrid      Int
  vancouver   Int
  london      Int
}
